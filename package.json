{
  "name": "yajsondiff",
  "description": "Yet another JSON diff utility, for generating and applying patches",
  "version": "1.0.2",
  "license": "MIT",
  "keywords": [
    "diff",
    "difference",
    "compare",
    "change-tracking",
    "typescript"
  ],
  "author": "Tristan Siegel <tristan@bustle.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/bustle/yajsondiff.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc -b tsconfig-build.json",
    "test": "npm run unit-test && npm run lint",
    "unit-test": "TS_NODE_TRANSPILE_ONLY=true mocha --opts mocha.opts",
    "lint": "tslint lib/*.ts",
    "format": "tslint lib/*.ts --fix"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/lodash.clonedeep": "^4.5.4",
    "@types/mocha": "^5.2.5",
    "deep-equal": "^1.0.1",
    "deep-freeze": "0.0.1",
    "eslint": "^4.19.1",
    "eslint-plugin-mocha": "^5.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  }
}
